        <file src="Xamarin.RangeSlider.iOS\bin\Release\Xamarin.RangeSlider.iOS.dll" target="lib\net7.0-ios16.1\Xamarin.RangeSlider.iOS.dll" />
        <file src="Xamarin.RangeSlider.iOS\bin\Release\Xamarin.RangeSlider.iOS.pdb" target="lib\net7.0-ios16.1\Xamarin.RangeSlider.iOS.pdb" />

        <file src="Xamarin.RangeSlider.iOS\bin\Release\Xamarin.RangeSlider.Common.dll" target="lib\net7.0-ios16.1\Xamarin.RangeSlider.Common.dll" />
        <file src="Xamarin.RangeSlider.iOS\bin\Release\Xamarin.RangeSlider.Common.pdb" target="lib\net7.0-ios16.1\Xamarin.RangeSlider.Common.pdb" />
        <file src="Xamarin.RangeSlider.iOS\Resources\*.png" target="content\net7.0-ios16.1\Resources" />
