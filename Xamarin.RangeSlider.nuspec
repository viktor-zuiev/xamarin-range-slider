<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Xamarin.RangeSlider</id>
        <version><%version%></version>
        <title>Xamarin.RangeSlider</title>
        <authors>Artur Shamsutdinov</authors>
        <projectUrl>https://github.com/halkar/xamarin-range-slider</projectUrl>
